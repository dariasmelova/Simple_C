/* a program that asks the user to enter two integers, obtains the
numbers from the user, then prints the larger number followed by the words “is larger.” If the
numbers are equal, print the message “These numbers are equal.” */

#include <stdio.h>

// function main begins program execution
int main(void)
{
	int num1, num2;

	printf("Enter two integers:\n");

	scanf_s("%d%d", &num1, &num2);

	if (num1 > num2)
		printf("%d is larger\n", num1);

	if (num1 < num2)
		printf("%d is less\n", num1);

	if (num1 == num2)
		printf("These numbers are equal");

} // end function main

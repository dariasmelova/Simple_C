// a program that reads an integer and determines and prints whether itâ€™s odd or even

#include <stdio.h>

// function main begins program execution
int main(void)
{
	int integer;

	printf("Enter an integer\n");
	printf("and I will determine whether it is odd or even\n");

	scanf_s("%d", &integer);

	if (integer % 2 == 0)
		printf("Even");

	if (integer % 2 != 0)
		printf("Odd");

} // end function main

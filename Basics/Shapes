// a program that prints shapes with asterisks
#include <stdio.h>

// function main begins program execution 
int main(void)
{
	printf("*********\n");
	printf("*       *\n");
	printf("*       *\n");
	printf("*       *\n");
	printf("*       *\n");
	printf("*       *\n");
	printf("*       *\n");
	printf("*       *\n");
	printf("*********\n");

	printf("   ***   \n");
	printf(" *    *  \n");
	printf("*       *\n");
	printf("*       *\n");
	printf("*       *\n");
	printf("*       *\n");
	printf("*       *\n");
	printf(" *    *  \n");
	printf("   ***   \n");

	printf("    *    \n");
	printf("   ***   \n");
	printf("  *****  \n");
	printf("    *    \n");
	printf("    *    \n");
	printf("    *    \n");
	printf("    *    \n");
	printf("    *    \n");

	printf("    *    \n");
	printf("   * *   \n");
	printf("  *   *  \n");
	printf(" *     * \n");
	printf("*       *\n");
	printf(" *     * \n");
	printf("  *   *  \n");
	printf("   * *   \n");
	printf("    *    \n");

} // end function main 

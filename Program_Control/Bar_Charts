/* a program that reads five numbers (each between 1 and 30);
for each number a line containing that number of adjacent asterisks is printed */

#include <stdio.h>

int main(void) {
	int a, number, counter;

	for (counter = 1; counter <= 5; counter++) {
		printf("Enter any number from 1 to 30: \n");
		scanf_s("%d", &number);

		for (a = 1; a <= number; a++) {
			printf("*");
		}
		printf("\n");
	}
}

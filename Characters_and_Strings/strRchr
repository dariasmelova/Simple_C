// a program that demonstrates function strrchr

#include <stdio.h>
#include <string.h>

int main(void)
{
	const char* string1 = "Secret or success is sincerity";

	int character = 's'; 

	printf("%s\n%s'%c'%s\"%s\"\n",
		"The remainder of string1 beginning with the",
		"last occurrence of character ", character,
		" is: ", strrchr(string1, character));
}

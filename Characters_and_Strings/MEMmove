// a program that demonstrates function memmove

#include <stdio.h>
#include <string.h>

int main(void)
{
    char x[] = "James Bond"; 

    printf("%s%s\n", "The string in array x before memmove is: ", x);
    printf("%s%s\n", "The string in array x after memmove is: ",
        (char*)memmove(x, &x [6], 5));
}

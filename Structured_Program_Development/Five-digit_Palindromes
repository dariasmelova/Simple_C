// a program that reads in a five-digit integer and determines whether or not itâ€™s a palindrome

#include <stdio.h>

int main(void)
{
	int number;
	printf("Enter five-digit number: \n");
	scanf_s("%d", &number);

	if ((number % 10) != (number / 10000))
		printf("This is not a palindrome");
	else {
		if (((number / 10) % 10) == ((number / 1000) % 10))
			printf("This is a palindrome");
		else printf("This is not a palindrome");
	}
}
